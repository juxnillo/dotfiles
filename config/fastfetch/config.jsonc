{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "type": "auto",
    "source": "/home/wan/.config/fastfetch/pngs/kasumi.png",
    "height": 20,
    "width": 40,
  },

  "modules": [
    "break",
    {
      "type": "custom",
      "format": "           \u001b[91m鳥のように自由であれ",
    },
    {
      "type": "custom",
      "format": "\u001b[91m● ━━━━━━━━━━━━━━ \u001b[97m𝘏𝘢𝘳𝘥𝘸𝘢𝘳𝘦\u001b[91m ━━━━━━━━━━━━━ ●\u001b[0m",
    },
    {
      "type": "os",
      "key": "   :",
      "keyColor": "white",
    },
    {
      "type": "kernel",
      "key": "   :",
      "keyColor": "white",
    },
    {
      "type": "cpu",
      "key": "  󰻠 :",
      "showPeCoreCount": true,
      "format": "{1}",
      "keyColor": "white",
    },
    {
      "type": "gpu",
      "key": "  󰍛 :",
      "keyColor": "white",
      "format": "{name}",
    },
    {
      "type": "memory",
      "key": "   :",
      "keyColor": "white",
    },
    {
      "type": "custom",
      "format": "\u001b[91m● ━━━━━━━━━━━━━━ \u001b[97m𝘚𝘰𝘧𝘵𝘸𝘢𝘳𝘦\u001b[91m ━━━━━━━━━━━━━ ●\u001b[0m",
    },
    {
      "type": "packages",
      "key": "  󰏖 :",
      "keyColor": "white",
    },
    {
      "type": "shell",
      "key": "   :",
      "keyColor": "white",
    },
    {
      "type": "terminal",
      "key": "   :",
      "keyColor": "white",
    },
    {
      "type": "de",
      "key": "  󰍺 :",
      "keyColor": "white",
    },
    {
      "type": "command",
      "key": "   :",
      "keyColor": "white",
      "text": "birth_install=$(stat -c %W /); current=$(date +%s); time_progression=$((current - birth_install)); days_difference=$((time_progression / 86400)); echo $days_difference days",
    },
    {
      "type": "uptime",
      "key": "  󰃰 :",
      "keyColor": "white",
    },
    {
      "type": "custom",
      "format": "\u001b[91m● ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━ ●\u001b[0m",
    },
    {
      "type": "custom",
      "format": "                 \u001b[97m󰫶 󰬂󰬀󰫲\u001b[91m 󰫮󰫿󰫰󰫵 ",
    },
  ],
}
